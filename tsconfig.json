{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ],
    "outDir": "./dist/out-tsc",
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "earthquake-eventpages-lib": [
        "../dist/earthquake-eventpages-lib"
      ],
      "earthquake-eventpages-lib/*": [
        "../dist/earthquake-eventpages-lib/*"
      ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}